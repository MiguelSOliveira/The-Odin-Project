<h1>Users!</h1>

<h2>Registered Users:</h2>
<% @users.each do |user| %>
  <%= user.name %>
  <%= user.email %>
  <%= link_to "Show", user_path(user.id) %>
  <%= link_to "Edit", edit_user_path(user.id) %>
  <%= link_to "Destroy", user_path(user.id), :method => :delete %>
  <br/>
<% end %>

<%= link_to 'New', new_user_path %>
